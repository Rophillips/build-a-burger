<h1>Burgers to devour</h1>
<ol>
    {{#each burgers}}
    {{#unless devoured}}
    <li>
        {{burger_name}}
        <button data-id={{id}}>Eat the burger</button> 
    </li>
{{/unless}}
{{/each}}
</ol>

<h1>Burgers you've devoured</h1>
<ol>
    {{#each burgers}}
    {{#if devoured}}
    <li>
        {{burger_name}}
    </li>
{{/if}}
{{/each}}
</ol>